# The URL for SVN repos
export SVN_URL=""

# Format of SVN repos. {} is replaced with the project name
export SVN_REPO_URL="${SVN_URL}/{}/repos"

# Parent folder of SVN repos
export SVN_REPO_ROOT_DIR=""

# SVN username
export SVN_USER=""

# SVN password
export SVN_PASS=""

# Colon separated string of SVN repo names
export SVN_REPOS=""

# Any extra project directories that shouldn't be included in the repo
export CW_PROJECT_DIRS=""
