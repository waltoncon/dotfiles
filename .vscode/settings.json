{
    "conventionalCommits.scopes": [
        "install",
        "shell",
        "git",
        "npm",
        "zsh",
        "subversion",
        "wget"
    ],
    "files.associations": {
        "**/npm/.config/npm/npmrc": "ini",
        "**/shell/.config/shell/alias": "shellscript",
        "**/shell/.config/shell/env": "shellscript",
        "**/shell/.config/shell/func": "shellscript",
        "**/shell/.config/shell/init": "shellscript",
        "**/subversion/.config/subversion/servers": "ini",
    }
}